FUNCTION abs_divider_lbg (denominator[4..0], numerator[41..0])
RETURNS ( quotient[41..0], remainder[4..0]);

--synthesis_resources = lut 368 
SUBDESIGN lpm_divide_8so
( 
	denom[4..0]	:	input;
	numer[41..0]	:	input;
	quotient[41..0]	:	output;
	remain[4..0]	:	output;
) 
VARIABLE 
	divider : abs_divider_lbg;
	numer_tmp[41..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
